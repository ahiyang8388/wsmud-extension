{
  "name": "wsmud extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "an extension for wsmud",
  "content_scripts": [
    {
      "matches": ["http://game.wsmud.com/*"],
      "js": ["wsmud.js"],
	  "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "script/addon.js"
  ]
}